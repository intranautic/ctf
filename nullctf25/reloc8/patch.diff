index 22707f30836..07b4007829d 100644
--- a/src/builtins/builtins-array.cc
+++ b/src/builtins/builtins-array.cc
@@ -1742,5 +1742,112 @@ BUILTIN(ArrayConcat) {
   return Slow_ArrayConcat(&args, species, isolate);
 }
 
+BUILTIN(ArrayReloc8) {
+  HandleScope scope(isolate);
+
+  if (args.length() != 3) {
+    THROW_NEW_ERROR_RETURN_FAILURE(
+      isolate, 
+      NewTypeError(
+        MessageTemplate::kPlaceholderOnly,
+        isolate->factory()->NewStringFromAsciiChecked("invalid number of arguments")
+      )
+    );
+  }
+
+  Handle<Object> receiver = args.receiver();
+  if (!IsJSArray(*receiver) || !HasOnlySimpleReceiverElements(isolate, Cast<JSArray>(*receiver))) {
+    THROW_NEW_ERROR_RETURN_FAILURE(
+      isolate, 
+      NewTypeError(
+        MessageTemplate::kPlaceholderOnly,
+        isolate->factory()->NewStringFromAsciiChecked("invalid type")
+      )
+    );
+  }
+
+  Handle<JSArray> array = Cast<JSArray>(receiver);
+
+  uint32_t array_len = Object::NumberValue(array->length());
+
+  auto get_index_arg = [&](uint32_t index) -> Maybe<uint32_t> {
+    Handle<Object> obj;
+    ASSIGN_RETURN_ON_EXCEPTION_VALUE(
+      isolate,
+      obj,
+      Object::ToNumber(isolate, args.at(index)),
+      Nothing<uint32_t>()
+    );
+    return Just(static_cast<uint32_t>(Object::NumberValue(*obj)));
+  };
+  auto get_from_val = [&]() { return get_index_arg(1); };
+  auto get_to_val = [&]() { return get_index_arg(2); };
+
+  #define DECLARE_ARGS() \
+    uint32_t from_val; \
+    if (!get_from_val().To(&from_val)) { \
+      THROW_NEW_ERROR_RETURN_FAILURE( \
+        isolate, \
+        NewTypeError( \
+          MessageTemplate::kPlaceholderOnly, \
+          isolate->factory()->NewStringFromAsciiChecked("`from` is not a number") \
+        ) \
+      ); \
+    } \
+    \
+    uint32_t to_val; \
+    if (!get_to_val().To(&to_val)) { \
+      THROW_NEW_ERROR_RETURN_FAILURE( \
+        isolate, \
+        NewTypeError( \
+          MessageTemplate::kPlaceholderOnly, \
+          isolate->factory()->NewStringFromAsciiChecked("`to` is not a number") \
+        ) \
+      ); \
+    }
+
+  {
+    DECLARE_ARGS();
+
+    if (from_val < 0 || from_val >= array_len) {
+      THROW_NEW_ERROR_RETURN_FAILURE(
+        isolate,
+        NewTypeError(
+          MessageTemplate::kPlaceholderOnly,
+          isolate->factory()->NewStringFromAsciiChecked("invalid from")
+        )
+      );
+    }
+
+    if (to_val < 0 || to_val >= array_len) {
+      THROW_NEW_ERROR_RETURN_FAILURE(
+        isolate,
+        NewTypeError(
+          MessageTemplate::kPlaceholderOnly,
+          isolate->factory()->NewStringFromAsciiChecked("invalid to")
+        )
+      );
+    }
+  }
+  
+  {
+    DECLARE_ARGS();
+
+    if (IsDoubleElementsKind(array->GetElementsKind())) {
+      Tagged<FixedDoubleArray> elements = Cast<FixedDoubleArray>(array->elements());
+      double value = elements->get_scalar(from_val);
+      elements->set(to_val, value);
+    } else {
+      Tagged<FixedArray> elements = Cast<FixedArray>(array->elements());
+      Handle<Object> value = handle(elements->get(from_val), isolate);
+      elements->set(to_val, *value);
+    }
+  }
+
+  
+  return ReadOnlyRoots(isolate).undefined_value();
+  #undef DECLARE_ARGS
+}
+
 }  // namespace internal
 }  // namespace v8
