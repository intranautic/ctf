service ctf
{
    disable = no
    socket_type = stream
    protocol = tcp
    wait = no
    user = ctf
    type = UNLISTED
    bind = 0.0.0.0
    port = 1337
    server = /home/ctf/run_challenge.sh
    per_source      = 10 # the maximum instances of this service per source IP address
    rlimit_as     = 1024M # the Address Space resource limit for the service
}
